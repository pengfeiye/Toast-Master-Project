<div class="form">
  <%= form_with scope: :threew, url: update_wh_path(@threew.id), class: "form-horizontal", method: :PUT do |form| %>
  <h1><%=@threew.title%></h1>
  <div class="form-group">
  		<%= form.label :details, "Details:" %>
  		<%= form.text_area(:details, class: 'form-control') %>
  </div>
  <div class="form-group">
  		<%= form.submit('Submit', class: 'btn-sm btn-primary') %>
  </div>
  <% end %>
</div>
